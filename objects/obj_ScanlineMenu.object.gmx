            <string>///Main

//Selection

if (!instance_exists(obj_Fade))
{
    if (keyboard_check_pressed(ord("W")))
    {
        audio_play_sound(snd_MenuSelect,0,false);
        selection -= 1;
    }
    if (keyboard_check_pressed(ord("S")))
    {
        audio_play_sound(snd_MenuSelect,0,false);
        selection += 1;
    }
}

if (selection == -1) selection = 1;
if (selection == 2) selection = 0;

if (instance_exists(obj_Scanlines))
{
    if (selection = 0) obj_Scanlines.visible = false;
    if (selection = 1) obj_Scanlines.visible = true;
}

//Go To Next Room

if (!instance_exists(obj_Fade))
{
    if ((keyboard_check_pressed(ord("E"))) or (keyboard_check_pressed(vk_enter)))
    {
        if (selection == 0) audio_play_sound(snd_ButtonNo,0,false);
        if (selection == 1) audio_play_sound(snd_ButtonYes,0,false);
        fade = instance_create(x,y,obj_Fade);
        fade.nextRoom = rm_Staff;
    }
}
</string>
