          <argument>
            <kind>1</kind>
            <string>///Draw

//Animation

imageIndex += imageSpeed;
if (imageIndex &gt;= 2) imageIndex -= 2;

draw_sprite(spriteIndex,imageIndex,752,206);

//Draw Background

draw_sprite_ext(spr_WeaponSelect,0,0,0,2,2,image_angle,image_blend,image_alpha);

//Draw Set Values

draw_set_font(fnt_Alien);
draw_set_color(make_color_rgb(140,0,255));
var arrayEnd = array_length_1d(text);

//Get character

if (!instance_exists(obj_Fade))
{
    if (keyboard_check_pressed(ord("W")))
    {
        audio_play_sound(snd_MenuSelect,0,false);
        character -= 1;
    }
    if (keyboard_check_pressed(ord("S")))
    {
        audio_play_sound(snd_MenuSelect,0,false);
        character += 1;
    }
}
if (character == -1) character = arrayEnd - 1;
if (character == arrayEnd) character = 0;

//Draw Position

selectedY = startY + - (character * 64)
xx = lerp(xx,selectedX,spd);
yy = lerp(yy,selectedY,spd);

//Draw Text

for (array = 0; array &lt; arrayEnd; array += 1)
{
    if (array == character) textAlpha = 1;
    if ((array == character-1) or (array == character+1)) textAlpha = .60;
    if ((array == character-2) or (array == character+2)) textAlpha = .40;
    if ((array == character-3) or (array == character+3)) textAlpha = .20;
    if ((array &lt; character-3) or (array &gt; character+3)) textAlpha = 0;
    draw_set_alpha(textAlpha);
    draw_text(xx,yy + (array * 64),text[array]);
    draw_set_alpha(1);
}

draw_set_font(fnt_Chat);
draw_set_halign(fa_center);
draw_text(753,416,description);
draw_set_halign(fa_left);

//Go Back

if (!instance_exists(obj_Fade))
{
    if ((keyboard_check_pressed(vk_enter)) or (keyboard_check_pressed(ord("E"))))
    {
        if (selectable)
        {
            audio_play_sound(snd_ButtonYes,0,false);
            global.weapon = character;
            fade = instance_create(x,y,obj_Fade);
            fade.nextRoom = rm_Overworld1;
            scr_Save();
        }
        else
        {
            audio_play_sound(snd_ButtonNo,0,false);
        }
    }
}
