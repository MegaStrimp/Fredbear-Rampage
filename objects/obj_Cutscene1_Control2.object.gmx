            <string>///Alarms

//Switch Text

if(alarm_SwitchText != -1)
{
    alarm_SwitchText = max(0,alarm_SwitchText-1);
    if(alarm_SwitchText == 0)
    {
        switch (state)
        {
            case 0:
            text = "Placing those adverts took all day.";
            sound = snd_Fredbear_Cutscene2;
            break;
            case 1:
            text = "Perhaps I went a bit overboard#though...";
            sound = snd_Fredbear_Cutscene3;
            break;
            case 2:
            text = "Oh shoot, It's almost nighttime,#I must hurry.";
            sound = snd_Fredbear_Cutscene4;
            break;
            case 3:
            text = "I have to send a letter to Travis#Scott and record my newest single.";
            sound = snd_Fredbear_Cutscene5;
            break;
        }
        if (state &lt; 4)
        {
            objText = instance_create(view_xview + 90 + sprite_get_width(spr_Portrait_Fredbear),view_yview + 45,obj_Npc_Text);
            objText.text = text;
            objText.state = 1;
            objText.textSpeed = 30;
            objText.text_halign = fa_left;
            objText.textbox = true;
            objText.textboxPortrait = spr_Portrait_Fredbear;
            objText.sound = sound;
        }
        if (state == 5)
        {
            if (instance_exists(obj_Cutscene1_Car)) obj_Cutscene1_Car.xDestination = room_width + 400;
        }
        if (state == 7)
        {
            fade = instance_create(x,y,obj_Fade);
            fade.nextRoom = rm_Cutscene1_Scene3;
            fade.b = 0.01;
            if (audio_is_playing(mus_MistyRain)) audio_stop_sound(mus_MistyRain);
            audio_play_sound(mus_Dreams,0,false);
        }
        state += 1;
        if (state &lt; 5) alarm_SwitchText = -1;
        if (state == 5) alarm_SwitchText = 30;
        if (state == 6) alarm_SwitchText = 60;
    }
}
